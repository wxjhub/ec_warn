<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.vprisk.mnt.entity.TransactionTimeConfig" table="MNT_TRANSACTION_TIME_CONFIG">
		<id name="uuid" type="java.lang.String" column="UUID">
			<generator class="uuid"></generator>
		</id>
		<property name="transactionCode" type="java.lang.String" column="TRANSACTION_CODE"></property>
		<property name="transactionName" type="java.lang.String" column="TRANSACTION_NAME"></property>
		<property name="countCycle" type="java.lang.Integer" column="count_Cycle"></property>
		<property name="transactionTime" type="java.lang.Integer" column="transaction_Time"></property>
		<property name="transactionTimeValueY" type="java.lang.Integer" column="transaction_Time_Value_Y"></property>
		<property name="transactionTimeValueG" type="java.lang.Integer" column="transaction_Time_Value_G"></property>
		
		<property name="warningLevl" type="java.lang.Integer" column="WARNING_LEVL"></property>
		<property name="col1" type="java.lang.String" column="COL1"></property>
		<property name="col2" type="java.lang.String" column="COL2"></property>
		<property name="col3" type="java.lang.String" column="COL3"></property>
		
	</class>
</hibernate-mapping>
