<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.vprisk.mnt.entity.ApplyProSoftConfig" table="MNT_APPLY_PRO_SOFT_CONFIG">
		<id name="uuid" type="java.lang.String" column="UUID">
			<generator class="uuid"></generator>
		</id>
		<property name="hostName" type="java.lang.String" column="HOST_NAME"></property>
		<property name="localIp" type="java.lang.String" column="LOCAL_IP"></property>
		<property name="serverName" type="java.lang.String" column="SERVER_NAME"></property>
		<property name="userName" type="java.lang.String" column="USER_NAME"></property>
		<property name="maxCourseNum" type="java.lang.Integer" column="MAX_COURSE_NUM"></property>
		<property name="warningLevl" type="java.lang.Integer" column="WARNING_LEVL"></property>
		<property name="remark" type="java.lang.String" column="REMARK"></property>
		<property name="col1" type="java.lang.String" column="COL1"></property>
		<property name="col2" type="java.lang.String" column="COL2"></property>
		<property name="col3" type="java.lang.String" column="COL3"></property>
		
		<property name="courseNum" type="java.lang.Integer" column="COURSE_NUM"></property>
		<property name="collectDate" type="java.lang.String" column="COLLECT_DATE"></property>
		<property name="courseStatus" type="java.lang.String" column="COURSE_STATUS"></property>
		
	</class>
</hibernate-mapping>
