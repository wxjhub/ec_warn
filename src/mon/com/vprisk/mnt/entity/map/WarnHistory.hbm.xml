<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.vprisk.mnt.entity.WarnHistory" table="MNT_WARN_HISTORY_VALUE">
		<id name="uuid" type="java.lang.String" column="UUID">
			<generator class="uuid"></generator>
		</id>
		<property name="recordName" type="java.lang.String" column="RECORD_NAME"></property>
		<property name="pointName" type="java.lang.String" column="POINT_NAME"></property>
		<property name="pointIp" type="java.lang.String" column="POINT_IP"></property>
		<property name="warnSort" type="java.lang.String" column="WARN_SORT"></property>
		<property name="warnLevel" type="java.lang.Integer" column="WARN_LEVEL"></property>
		<property name="recordDate" type="java.lang.String" column="RECORD_DATE"></property>
		<property name="recordDescription" type="java.lang.String" column="RECORD_DESCRIPTION"></property>
		<property name="remark" type="java.lang.String" column="REMARK"></property>
		<property name="notificationPerson" type="java.lang.String" column="NOTIFICATION_PERSON"></property>
		<property name="confirmationPerson" type="java.lang.String" column="CONFIRMATION_PERSON"></property>
		<property name="department" type="java.lang.String" column="DEPARTMENT"></property>
		<property name="endDate" type="java.lang.String" column="END_DATE"></property>
		<property name="col1" type="java.lang.String" column="COL1"></property>
		<property name="col2" type="java.lang.String" column="COL2"></property>
		<property name="col3" type="java.lang.String" column="COL3"></property>
		
		
	</class>
</hibernate-mapping>
