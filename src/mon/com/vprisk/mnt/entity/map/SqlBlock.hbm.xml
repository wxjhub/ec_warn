<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.vprisk.mnt.entity.SqlBlock" table="MNT_SQL_BLOCK">
		<id name="uuid" type="java.lang.String" column="UUID">
			<generator class="uuid"></generator>
		</id>
		<property name="username" type="java.lang.String" column="USERNAME"></property>
		<property name="sid" type="java.lang.String" column="SID"></property>
		<property name="event" type="java.lang.String" column="EVENT"></property>
		<property name="sqlId" type="java.lang.String" column="SQL_ID"></property>
		<property name="command" type="java.lang.String" column="COMMAND"></property>
		<property name="blockingInstance" type="java.lang.String" column="BLOCKING_INSTANCE"></property>
		<property name="blockingSession" type="java.lang.String" column="BLOCKING_SESSION"></property>
		<property name="lastCallEt" type="java.lang.String" column="LAST_CALL_ET"></property>
		<property name="status" type="java.lang.String" column="STATUS"></property>
		<property name="waitClass" type="java.lang.String" column="WAIT_CLASS"></property>
		<property name="collectDate" type="java.lang.String" column="COLLECT_DATE"></property>
		<property name="warningLevl" type="java.lang.Integer" column="WARNING_LEVL"></property>
		<property name="remark" type="java.lang.String" column="REMARK"></property>
		<property name="col1" type="java.lang.String" column="COL1"></property>
		<property name="col2" type="java.lang.String" column="COL2"></property>
		<property name="col3" type="java.lang.String" column="COL3"></property>
		
	</class>
</hibernate-mapping>
