<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.vprisk.mnt.entity.WarnBatchConfig" table="MNT_WARN_BATCH_CONFIG">
		<id name="uuid" type="java.lang.String" column="UUID">
			<generator class="uuid"></generator>
		</id>
		<property name="batchId" type="java.lang.String" column="BATCH_ID"></property>
		<property name="taskSetId" type="java.lang.String" column="TASK_SET_ID"></property>
		<property name="taskId" type="java.lang.String" column="TASK_ID"></property>
		<property name="warnLevl" type="java.lang.Integer" column="WARN_LEVL"></property>
		<property name="warnContent" type="java.lang.String" column="WARN_CONTENT"></property>
		<property name="warnContentValue" type="java.lang.String" column="WARN_CONTENT_VALUE"></property>
		<property name="col1" type="java.lang.String" column="COL1"></property>
		<property name="col2" type="java.lang.String" column="COL2"></property>
		<property name="col3" type="java.lang.String" column="COL3"></property>
	</class>
</hibernate-mapping>
