<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.vprisk.mnt.entity.LocalNetWork" table="MNT_WARN_LOCAL_NET_WORK">
		<id name="uuid" type="java.lang.String" column="UUID">
			<generator class="uuid"></generator>
		</id>
		<property name="hostName" type="java.lang.String" column="HOST_NAME"></property>
		<property name="hostIp" type="java.lang.String" column="HOST_IP"></property>
		<property name="hostPort" type="java.lang.String" column="HOST_PORT"></property>
		<property name="netMeaning" type="java.lang.String" column="NET_MEANING"></property>
		<property name="collectDate" type="java.lang.String" column="COLLECT_DATE"></property>
		<property name="status" type="java.lang.String" column="STATUS"></property>
		<property name="warningLevl" type="java.lang.Integer" column="WARNING_LEVL"></property>
		<property name="col1" type="java.lang.String" column="COL1"></property>
		<property name="col2" type="java.lang.String" column="COL2"></property>
		<property name="col3" type="java.lang.String" column="COL3"></property>
	</class>
</hibernate-mapping>
